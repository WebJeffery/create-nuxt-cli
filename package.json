{
  "name": "create-nuxt-app",
  "version": "0.1.0",
  "description": "一个用于快速创建企业级 Nuxt3 全栈应用的命令行工具",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "clean": "turbo run clean",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "prettier": "^3.2.5",
    "turbo": "^1.12.4",
    "typescript": "^5.3.3"
  },
  "packageManager": "pnpm@8.15.1",
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "nuxt",
    "nuxt3",
    "cli",
    "create-nuxt",
    "template"
  ],
  "author": "",
  "license": "MIT"
}
